{
    "manifest_version": 2,
    "name": "Chrome Blocker",
    "description": "Blocks websites",
    "version": "1.0.0",
    "background": {
        "scripts": ["./components/background.js"]
    },
    "icons": {"128": "icon128.png"},
    "browser_action": {
        "default_icon": "icon19.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js; object-src 'self'",
    "permissions": ["activeTab", "storage", "webRequest", "webRequestBlocking", "tabs", "<all_urls>"]
}