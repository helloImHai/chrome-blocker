{
    "manifest_version": 3,
    "name": "Chrome Blocker",
    "description": "Blocks websites",
    "version": "1.1.0",
    "icons": { 
        "19": "icon19.png",
        "128": "icon128.png" 
    },
    "action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "./components/background.js"
    },
    "host_permissions": ["https://*/*"],
    "permissions": ["tabs", "scripting", "storage"]
}
